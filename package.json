{
  "name": "@zazuko/cube-creator",
  "private": true,
  "scripts": {
    "lint": "eslint . --ext .ts,.vue,.tsx --quiet --ignore-path .gitignore",
    "test": "c8 -o coverage/apis mocha --recursive apis/**/*.test.ts",
    "test:cli": "c8 -o coverage/cli mocha --recursive cli/**/*.test.ts",
    "seed-data": "dotenv -e .local.env -- bash -c \"ts-node packages/testing/index.ts -i fuseki/sample-ubd.trig\""
  },
  "workspaces": [
    "cli",
    "ui",
    "apis/*",
    "packages/*"
  ],
  "devDependencies": {
    "@tpluscode/eslint-config": "^0.1.1",
    "@typescript-eslint/eslint-plugin": "^4.4.1",
    "@typescript-eslint/parser": "^4.4.1",
    "c8": "^7.3.5",
    "dotenv": "^8.2.0",
    "eslint": "^7.11.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-vue": "^7.1.0",
    "standard": "^14.3.4",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3"
  },
  "resolutions": {
    "@rdfjs/express-handler": "tpluscode/express-handler#dataset-multiple-times"
  },
  "mocha": {
    "extension": "ts",
    "recursive": true,
    "watch-files": [
      "./**/*.ts"
    ],
    "require": [
      "mocha-setup.js"
    ]
  },
  "c8": {
    "all": true,
    "reporter": "lcov",
    "exclude": [
      "node_modules",
      "ui",
      "typings",
      "apis/*/test",
      "cli/test"
    ]
  }
}
