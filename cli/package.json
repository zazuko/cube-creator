{
  "name": "@cube-creator/cli",
  "version": "0.0.0",
  "description": "Runs a pipeline to create an RDF data cube",
  "main": "index.js",
  "scripts": {
    "postinstall": "mkdir -p input; mkdir -p output",
    "transform": "dotenv -- yarn start",
    "start": "ts-node index.ts transform --from s3 --project \"$TRANSFORM_PROJECT\" --debug --enable-buffer-monitor -v s3endpoint=$S3_ENDPOINT -v s3bucket=$S3_BUCKET -v graph-store-endpoint=$GRAPH_STORE_ENDPOINT -v graph-store-user=$GRAPH_STORE_USER -v graph-store-password=$GRAPH_STORE_PASSWORD -v graph=$TRANSFORM_GRAPH --auth-issuer $AUTH_ISSUER --auth-client-id $AUTH_CLIENT_ID --auth-client-secret $AUTH_CLIENT_SECRET",
    "build": "tsc"
  },
  "dependencies": {
    "@cube-creator/model": "*",
    "@rdfine/csvw": "^0.5.1",
    "@rdfjs/fetch-lite": "^2.0.1",
    "@rdfjs/namespace": "^1.1.0",
    "@tpluscode/rdf-ns-builders": "^0.4",
    "@tpluscode/rdfine": "^0.5.9",
    "alcaeus": "^1.0.0-RC.1",
    "aws-sdk": "^2.559.0",
    "barnard59": "^0.1.0",
    "barnard59-base": "^0.0.4",
    "barnard59-formats": "^0.0.7",
    "barnard59-graph-store": "^0.0.6",
    "clownface": "^1",
    "commander": "^4.1.1",
    "debug": "^4.1.1",
    "duplex-to": "^1.0.0",
    "get-stream": "^5.1.0",
    "isstream": "^0.1.2",
    "null-writable": "^1.0.5",
    "once": "^1.4.0",
    "rdf-ext": "^1.3.0",
    "readable-stream": "^3.6.0",
    "string-to-stream": "^3.0.1",
    "through2-reduce": "1.1.1"
  },
  "devDependencies": {
    "@types/clownface": "^1",
    "@types/debug": "^4.1.5",
    "@types/isstream": "^0.1.0",
    "@types/node-fetch": "^2.5.7",
    "@types/once": "^1.4.0",
    "@types/rdf-ext": "^1.3.5",
    "@types/rdf-js": "^4",
    "@types/rdfjs__formats-common": "^2.0.0",
    "@types/rdfjs__namespace": "^1.1.1",
    "@types/rdfjs__sink-map": "^1.0.0",
    "@types/readable-stream": "^2.3.9",
    "dotenv-cli": "^3.1.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zazuko/data-cube-curation.git"
  },
  "keywords": [
    "rdf",
    "data",
    "cube"
  ],
  "author": "Tomasz Pluskiewicz <tomasz.pluskiewicz@zazuko.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zazuko/data-cube-curation/issues"
  },
  "homepage": "https://github.com/zazuko/data-cube-curation/tree/master/cli#readme"
}
