    "@opentelemetry/sdk-node": "^0.35.1",
    "@opentelemetry/semantic-conventions": "^0.26.0",
    "@opentelemetry/tracing": "^0.24.0",
    "@rdfine/csvw": "^0.8.5",
    "@rdfine/prov": "^0.3.5",
    "@rdfine/schema": "^0.8.5",
    "@rdfine/shacl": "^0.10.5",
    "@sentry/node": "^6.2.0",
    "@sentry/tracing": "^6.2.0",
    "@tpluscode/rdf-ns-builders": "^4.3.0",
