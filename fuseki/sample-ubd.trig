<cube-project/ubd/dataset> void:inDataset <ubd-example> .
graph <cube-project/ubd/dataset> {
  <cube-project/ubd/dataset>
    a void:Dataset, dcat:Dataset, schema:Dataset ;
    schema:hasPart <https://environment.ld.admin.ch/foen/ubd/28/> ;
    cc:dimensionMetadata <cube-project/ubd/dimensions-metadata> ;
  .

  <https://environment.ld.admin.ch/foen/ubd/28/>
    a cube:Cube ;
    schema:dateCreated "2015-08-06"^^xsd:date;
    dcterms:creator <user> ;
  .
}

graph <cube-project/ubd/dimensions-metadata> {
  <cube-project/ubd/dimensions-metadata> a cc:DimensionsMetadataCollection ;
    schema:hasPart <cube-project/ubd/dimensions-metadata?dimension=https://environment.ld.admin.ch/foen/ubd/28/dimension%20year> ;
    schema:hasPart <cube-project/ubd/dimensions-metadata?dimension=https://environment.ld.admin.ch/foen/ubd/28/station> ;
    schema:hasPart <cube-project/ubd/dimensions-metadata?dimension=https://environment.ld.admin.ch/foen/ubd/28/aggregation> ;
    schema:hasPart <cube-project/ubd/dimensions-metadata?dimension=https://environment.ld.admin.ch/foen/ubd/28/pollutant> ;
    schema:hasPart <cube-project/ubd/dimensions-metadata?dimension=https://environment.ld.admin.ch/foen/ubd/28/unit-id> ;
  .

  <cube-project/ubd/dimensions-metadata?dimension=https://environment.ld.admin.ch/foen/ubd/28/shape/dimension%20year>
    schema:about <https://environment.ld.admin.ch/foen/ubd/28/dimension%20year> ;
    schema:name "Jahr"@de ;
    scale:scaleOfMeasure scale:Temporal ;
  .

  <cube-project/ubd/dimensions-metadata?dimension=https://environment.ld.admin.ch/foen/ubd/28/aggregation>
    schema:about <https://environment.ld.admin.ch/foen/ubd/28/aggregation> ;
  .
}

<cube-project/ubd/cube-data> void:inDataset <ubd-example> .
graph <cube-project/ubd/cube-data> {
  <https://environment.ld.admin.ch/foen/ubd/28/>
    a cube:Cube ;
    cube:observationConstraint  <https://environment.ld.admin.ch/foen/ubd/28/shape/> ;
    cube:observationSet         <https://environment.ld.admin.ch/foen/ubd/28/observation/> ;
  .

  <https://environment.ld.admin.ch/foen/ubd/28/observation/>
    a                 cube:ObservationSet ;
    cube:observation  <https://environment.ld.admin.ch/foen/ubd/28/observation/blBAS-1999-annualmean> ,
                      <https://environment.ld.admin.ch/foen/ubd/28/observation/blBAS-2002-annualmean> ;
  .

  <https://environment.ld.admin.ch/foen/ubd/28/observation/blBAS-2002-annualmean>
    a                cube:Observation ;
    cube:observedBy  <https://ld.stadt-zuerich.ch/> ;
    <https://environment.ld.admin.ch/foen/ubd/28/aggregation>     <https://environment.ld.admin.ch/foen/ubd/28/aggregation/annualmean> ;
    <https://environment.ld.admin.ch/foen/ubd/28/dimension/value> "6.1"^^xsd:float ;
    <https://environment.ld.admin.ch/foen/ubd/28/dimension/year>  "2002"^^xsd:gYear ;
    <https://environment.ld.admin.ch/foen/ubd/28/pollutant>       <https://environment.ld.admin.ch/foen/ubd/28/pollutant/so2> ;
    <https://environment.ld.admin.ch/foen/ubd/28/station>         <https://environment.ld.admin.ch/foen/ubd/28/station/blBAS> ;
    <https://environment.ld.admin.ch/foen/ubd/28/unit-id>         "µg/m3" ;
  .

<https://environment.ld.admin.ch/foen/ubd/28/observation/blBAS-1999-annualmean>
    a                cube:Observation ;
    cube:observedBy  <https://ld.stadt-zuerich.ch/> ;
    <https://environment.ld.admin.ch/foen/ubd/28/aggregation>     <https://environment.ld.admin.ch/foen/ubd/28/aggregation/annualmean> ;
    <https://environment.ld.admin.ch/foen/ubd/28/dimension/value> "5.4"^^xsd:float ;
    <https://environment.ld.admin.ch/foen/ubd/28/dimension/year>  "1999"^^xsd:gYear ;
    <https://environment.ld.admin.ch/foen/ubd/28/pollutant>       <https://environment.ld.admin.ch/foen/ubd/28/pollutant/so2> ;
    <https://environment.ld.admin.ch/foen/ubd/28/station>         <https://environment.ld.admin.ch/foen/ubd/28/station/blBAS> ;
    <https://environment.ld.admin.ch/foen/ubd/28/unit-id>         "µg/m3" ;
  .

  <https://environment.ld.admin.ch/foen/ubd/28/shape/>
    a            sh:NodeShape , cube:Constraint ;
    sh:closed    true ;
    sh:property  _:observedBy ;
    sh:property  _:rdfType ;
    sh:property  <https://environment.ld.admin.ch/foen/ubd/28/shape/dimension%20year>;
    sh:property  <https://environment.ld.admin.ch/foen/ubd/28/shape/station> ;
    sh:property  <https://environment.ld.admin.ch/foen/ubd/28/shape/aggregation> ;
    sh:property  <https://environment.ld.admin.ch/foen/ubd/28/shape/pollutant> ;
    sh:property  <https://environment.ld.admin.ch/foen/ubd/28/shape/unit-id> ;
  .

  <https://environment.ld.admin.ch/foen/ubd/28/shape/aggregation>
    sh:in        ( <https://environment.ld.admin.ch/foen/ubd/28/aggregation/annualmean> ) ;
    sh:maxCount  1 ;
    sh:minCount  1 ;
    sh:nodeKind  sh:IRI ;
    sh:path      <https://environment.ld.admin.ch/foen/ubd/28/aggregation> .

  _:observedBy
    sh:in        ( <https://ld.stadt-zuerich.ch/> ) ;
    sh:maxCount  1 ;
    sh:minCount  1 ;
    sh:nodeKind  sh:IRI ;
    sh:path      cube:observedBy .

  <https://environment.ld.admin.ch/foen/ubd/28/shape/pollutant>
    sh:in        ( <https://environment.ld.admin.ch/foen/ubd/28/pollutant/so2> ) ;
    sh:maxCount  1 ;
    sh:minCount  1 ;
    sh:nodeKind  sh:IRI ;
    sh:path      <https://environment.ld.admin.ch/foen/ubd/28/pollutant> .

  <https://environment.ld.admin.ch/foen/ubd/28/shape/dimension%2Fyear>
    sh:datatype  xsd:gYear ;
    sh:in        ( "1984"^^xsd:gYear "1985"^^xsd:gYear "1986"^^xsd:gYear "1987"^^xsd:gYear "1988"^^xsd:gYear "1989"^^xsd:gYear "1990"^^xsd:gYear "1991"^^xsd:gYear "1992"^^xsd:gYear "1993"^^xsd:gYear "1994"^^xsd:gYear "1995"^^xsd:gYear "1996"^^xsd:gYear "1997"^^xsd:gYear "1998"^^xsd:gYear "1999"^^xsd:gYear "2000"^^xsd:gYear "2001"^^xsd:gYear "2002"^^xsd:gYear "2003"^^xsd:gYear ) ;
    sh:maxCount  1 ;
    sh:minCount  1 ;
    sh:nodeKind  sh:Literal ;
    sh:path      <https://environment.ld.admin.ch/foen/ubd/28/dimension/year> .

  _:rdfType
    sh:in        ( cube:Observation ) ;
    sh:maxCount  1 ;
    sh:minCount  1 ;
    sh:nodeKind  sh:IRI ;
    sh:path      rdf:type .

  <https://environment.ld.admin.ch/foen/ubd/28/shape/unit-id>
    sh:datatype  xsd:string ;
    sh:in        ( "µg/m3" ) ;
    sh:maxCount  1 ;
    sh:minCount  1 ;
    sh:nodeKind  sh:Literal ;
    sh:path      <https://environment.ld.admin.ch/foen/ubd/28/unit-id> .

  <https://environment.ld.admin.ch/foen/ubd/28/shape/station>
    sh:in        ( <https://environment.ld.admin.ch/foen/ubd/28/station/blBAS> ) ;
    sh:maxCount  1 ;
    sh:minCount  1 ;
    sh:nodeKind  sh:IRI ;
    sh:path      <https://environment.ld.admin.ch/foen/ubd/28/station> .
}

